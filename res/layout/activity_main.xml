<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity" >

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="64dp"
        android:layout_height="fill_parent"
        android:background="#262729"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/main_rl_sy"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:duplicateParentState="true"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/laa1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@drawable/home"
                    android:duplicateParentState="true" />

                <TextView
                    android:id="@+id/textsy"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/laa1"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="14dp"
                    android:text="首页"
                    android:textColor="@color/bgtitle"
                    android:textSize="@dimen/textsmall" />
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/main_rl_gwc"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:duplicateParentState="true"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/igw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@drawable/shopping2"
                    android:duplicateParentState="true" />

                <TextView
                    android:id="@+id/textghc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/igw"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="14dp"
                    android:text="购物车"
                    android:textColor="#f3f2f2"
                    android:textSize="@dimen/textsmall" />
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/main_rl_pos1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"  >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:duplicateParentState="true"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/im_mess"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@drawable/message2"
                    android:duplicateParentState="true" />

                <TextView
                    android:id="@+id/textmes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/im_mess"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="14dp"
                    android:text="我的消息"
                    android:textColor="#f3f2f2"
                    android:textSize="@dimen/textsmall" />
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/main_rl_my"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"  >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:duplicateParentState="true"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/im_wd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:background="@drawable/mine2"
                    android:duplicateParentState="true" />

                <TextView
                    android:id="@+id/textwd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/textmes"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="14dp"
                    android:text="我的"
                    android:textColor="#f3f2f2"
                    android:textSize="@dimen/textsmall" />
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="60dp"
                android:id="@+id/set"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_marginBottom="47dp"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/mine_g" />

            </LinearLayout>

        </RelativeLayout>

    </LinearLayout>
<fragment 
    android:id="@+id/m_fragment"
     android:name="com.example.zf_pad.fragment.m_MianFragment"  
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"/>
</LinearLayout>